{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"buildCommand": "bun run build",
	"outputDirectory": "dist",
	"installCommand": "bun install",
	"framework": null,
	"devCommand": "bun run dev",
	"rewrites": [
		{
			"source": "/api/:path*",
			"destination": "/api"
		},
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	]
}
