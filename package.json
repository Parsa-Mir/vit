{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "routes:gen": "bunx @tanstack/router-cli generate",
    "routes:watch": "bunx @tanstack/router-cli watch",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts",
    "vercel:dev": "vercel dev",
    "vercel:build": "bun run build && vercel build",
    "vercel:preview": "bun run build && vercel dev"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/drei": "^9.121.5",
    "@react-three/fiber": "^9.0.0",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.134.13",
    "@tanstack/react-router-devtools": "^1.134.13",
    "@tanstack/router-devtools": "^1.134.13",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "hono": "^4.10.4",
    "lil-gui": "^0.21.0",
    "lucide-react": "^0.545.0",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.3.1",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest",
    "@tanstack/router-cli": "^1.134.13",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
